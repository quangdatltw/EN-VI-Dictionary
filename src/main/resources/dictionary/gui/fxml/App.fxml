<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane stylesheets="@../css/dark_bg.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionary.gui.control.AppController">
   <children>
      <VBox prefHeight="575.0" prefWidth="960.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane maxHeight="35.0" stylesheets="@../css/TaskBar.css" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                  <HBox fillHeight="false" maxHeight="35.0" prefHeight="35.0" prefWidth="990.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                     <children>
                        <Pane maxHeight="35.0" prefHeight="35.0" prefWidth="835.0" HBox.hgrow="ALWAYS">
                           <children>
                              <ImageView fitHeight="35.0" fitWidth="35.0" layoutX="6.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../App_icon.png" />
                                 </image>
                              </ImageView>
                           </children>
                        </Pane>
                        <Button mnemonicParsing="false" onMouseClicked="#minimize" prefHeight="35.0" prefWidth="42.0" stylesheets="@../css/Tab-GG.css" HBox.hgrow="ALWAYS">
                           <graphic>
                              <ImageView fitHeight="18.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../icon/Minimize.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="maximizeB" mnemonicParsing="false" onMouseClicked="#maximize" prefHeight="35.0" prefWidth="42.0" stylesheets="@../css/Tab-GG.css" HBox.hgrow="ALWAYS">
                           <graphic>
                              <ImageView fitHeight="18.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../icon/maximize1.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button cancelButton="true" mnemonicParsing="false" onMouseClicked="#close" pickOnBounds="false" prefHeight="35.0" prefWidth="42.0" HBox.hgrow="ALWAYS">
                           <graphic>
                              <ImageView fitHeight="11.0" fitWidth="11.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../icon/Close.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
            <TabPane fx:id="TabPane" focusTraversable="false" prefHeight="540.0" prefWidth="960.0" stylesheets="@../css/Tab-Pane.css" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
               <tabs>
                  <Tab fx:id="Tab_Find" text="Find">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../icon/Find.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Tab>
                  <Tab fx:id="Tab_GoogleTranslate" text=" Google Translate">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../icon/Translate.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Tab>
                  <Tab fx:id="Tab_AddRemoveUpdate" text="Add/Remove/Update">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../icon/Database.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Tab>
                  <Tab fx:id="Tab_Game" text="Game">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../icon/Game.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Tab>
               </tabs>
            </TabPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
